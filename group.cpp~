#include <vector>
#include <iostream>
#include <fstream>
#include <cstdlib>
 using namespace std;
 double mean(ifstream& fin);
 int main()
 {
 ifstream fin;
 fin.open("data1.txt");
 double val =0;
 cout << mean(fin) << endl;
 cin >> val;
 fin.close();
 return 0;
 }
 
double mean(ifstream& fin)
{
    int ary[20];
    string word;
    int val,size = 0;
    int ix=0;
    int sum = 0;
    int i = 0;
    for(i; i < 1; i++) 
    {
        fin >> word;
    }
        while(fin >> val)
    {
        ary[ix] = val;
        cout << ary[ix] << endl;
        ix++;
        size++;
    }
    
    for(i; i < size;i++)
    {
        sum += ary[i];
    }
   
    return sum/size ;
}
